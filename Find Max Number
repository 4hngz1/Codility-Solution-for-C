#include <stdio.h>
#include <stdlib.h>

int _max3_ (int number_1, int number_2, int number_3);

int _max3_ (int number_1, int number_2, int number_3)
{

	int maxVal = number_3 > number_2 ? number_3 : number_2;
	return  number_1 > maxVal ? number_1 : maxVal;
}


int main(void) {
	printf("Enter 3 Number:\n");
	fflush(stdout);

	int x, y, z;

	scanf("%d%d%d",&x,&y,&z);
	printf("Max Val:%d\r\n",_max3_(x,y,z));
	return EXIT_SUCCESS;
}
